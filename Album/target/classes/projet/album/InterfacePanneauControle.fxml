<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="projet.album.controller.PanneauController" fx:id="i_panneau" spacing="25.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
    <children>
        <AnchorPane>
            <children>
                <HBox alignment="BASELINE_CENTER" spacing="100.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
                    <children>
                        <Label fx:id="title" text="Nom d'album : " />
                        <Label fx:id="pages" text="Page : ">
                            <HBox.margin>
                                <Insets />
                            </HBox.margin>
                        </Label>
                    </children>
                    <opaqueInsets>
                        <Insets />
                    </opaqueInsets>
                </HBox>
            </children>
        </AnchorPane>
        <AnchorPane>
            <children>
                <HBox alignment="BASELINE_CENTER" spacing="100.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
                    <children>
                        <AnchorPane>
                            <children>
                                <HBox AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                    <children>
                                        <Button fx:id="b_prev" mnemonicParsing="false" onAction="#previous" text="&lt;" />
                                        <Button fx:id="b_next" mnemonicParsing="false" onAction="#next" text="&gt;" />
                                    </children>
                                </HBox>
                            </children>
                        </AnchorPane>
                        <AnchorPane>
                            <children>
                                <HBox AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                    <opaqueInsets>
                                        <Insets />
                                    </opaqueInsets>
                                    <children>
                                        <Button fx:id="b_add" mnemonicParsing="false" onAction="#ajouter" text="+" />
                                        <Button fx:id="b_remove" mnemonicParsing="false" text="-" />
                                    </children>
                                </HBox>
                            </children>
                        </AnchorPane>
                    </children>
                </HBox>
            </children>
        </AnchorPane>
    </children>
    <padding>
        <Insets top="25.0" />
    </padding>
</VBox>